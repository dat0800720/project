<tbody>
  <tr>
    <td><%= member.name %></td>
    <td>
        <%= member.employee_code %>
    </td>
    <td>
      <%= member.bithday&.strftime("%d/%m/%Y") %>
    </td>
    <td>
      <%= member.home_town %>
    </td>
    <td>
      <%= Constant::WORK_STATUS[member.work_status] %>
    </td>
    <td class="project-actions text-right">
      <% if current_user&.admin? %>
        <%= link_to member, class: "btn btn-primary btn-sm" do %>
          <i class="fas fa-eye"></i>
        <% end %>
        <%= link_to edit_member_path(member), class: "btn btn-primary btn-sm" do %>
          <i class="fas fa-edit"></i>
        <% end %>
        <%= link_to member, class: "btn btn-primary btn-sm", method: :delete, data: { confirm: t("boby.are_you_sure?") } do %>
          <i class="fas fa-trash"></i>
        <% end %>
      <% elsif current_user.member&.id == member.id %>
        <%= link_to member, class: "btn btn-primary btn-sm" do %>
          <i class="fas fa-eye"></i>
        <% end %>
        <%= link_to edit_member_path(member), class: "btn btn-primary btn-sm" do %>
          <i class="fas fa-edit"></i>
        <% end %>
      <% else %>
        <%= link_to member, class: "btn btn-primary btn-sm" do %>
          <i class="fas fa-eye"></i>
        <% end %>
      <% end %>
    </td>
  </tr>
</tbody>